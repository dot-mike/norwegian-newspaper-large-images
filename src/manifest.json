{
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "version": "1.0",
    "manifest_version": 2,
    "default_locale": "no",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://static.polarismedia.no/resources/min-js/plugins/imager.js*"
    ],
    "web_accessible_resources": [
        "includes/Imager.js"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [{
        "css": [
            "styles.css",
            "includes/zoom.css"
        ],
        "js": [
            "content.js",
            "includes/jquery.js",
            "includes/zoom.js",
            "includes/transition.js"
        ],
        "matches": [
            "https://www.morenytt.no/*"
        ]
    }],
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    }
}